{
  "title": "Daning·Swift",
  "author": "Daning",
  "language": "zh-hans",
  "styles": {
    "website": "./styles/website.css"
  },
  "links": {
    "sidebar": {
      "GitHub": "https://github.com/NeedQuiet",
      "个人博客": "https://daning.netlify.app/"
    }
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-sharing",
    "-highlight",
    "-livereload",
    "prism",
    "github",
    "anchor-navigation-ex",
    "expandable-chapters-small"
    
  ],
  "pluginsConfig": {
    "fontsettings": {
      "theme": "night",
      "family": "sans",
      "size": 1
    },
    "prism": {
      "css": ["prismjs/themes/prism-okaidia.css"],
      "lang": {
        "oc": "objectivec",
        "objc": "objectivec",
        "objective-c": "objectivec",
        "ObjectiveC": "objectivec",
        "sw": "swift"
      },
      "ignore": ["sequence"]
    },
    "github": {
      "url": "https://github.com/NeedQuiet/Swift_Learn_Gitbook"
    },
    "anchor-navigation-ex": {
      "showGoTop": true,
      "showLevel": false,
      "printLog": true
    }
  }
}
