{
  "title": "Daning·Swift",
  "author": "Daning",
  "description": "Swift精简版笔记",
  "language": "zh-hans",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-sharing",
    "js-sequence-diagram-full@>=0.3.1",
    "prism",
    "-highlight",
    "splitter",
    "github",
    "anchor-navigation-ex",
    "expandable-chapters-small",
    "-livereload"
  ],
  "pluginsConfig": {
    "prism": {
      "css": ["prismjs/themes/prism-okaidia.css"],
      "lang": {
        "oc": "objectivec",
        "objc": "objectivec",
        "objective-c": "objectivec",
        "ObjectiveC": "objectivec"
      },
      "ignore": ["sequence"]
    },
    "github": {
      "url": "https://github.com/NeedQuiet/Swift_Learn_Gitbook"
    },
    "anchor-navigation-ex": {
      "showGoTop": true,
      "showLevel": false,
      "printLog": true
    }
  }
}
